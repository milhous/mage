{
    "compilerOptions": {
      "module": "commonjs",
      "lib": ["webworker", "es2015", "es2017", "esnext", "dom"],
      "target": "es5",
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "outDir": "temp/vscode-dist",
      "forceConsistentCasingInFileNames": true,
      "downlevelIteration": true,
      "allowSyntheticDefaultImports": true,
      "allowJs": true,
      "sourceMap": true
    },
    "exclude": [
      "node_modules",
      "library",
      "local",
      "temp",
      "build",
      "settings"
    ]
  }