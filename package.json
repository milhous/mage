{
    "name": "milhous",
    "version": "1.0.0",
    "description": "主站2.0",
    "type": "module",
    "main": "index.js",
    "private": true,
    "repository": {
        "type": "git",
        "url": "http://gitlab.lan.com/front-end/website-v2.git"
    },
    "workspaces": [
        "packages/*",
        "mage-cli"
    ],
    "scripts": {
        "init": "mage init",
        "i18n": "mage i18n",
        "launch": "mage launch",
        "setup": "lerna init --independent && lerna clean --yes && lerna bootstrap",
        "update": "concurrently 'ncu -u --deep'",
        "start": "yarn workspaces foreach -vtR run start",
        "start:name": "cross-var yarn workspace @packages/$name run start",
        "start:batch": "yarn workspace @packages/$name run start",
        "build": "yarn workspaces foreach -vtR run build",
        "build:name": "cross-var yarn workspace @packages/$name run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "cross-var": "^1.1.0",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "npm-check-updates": "^13.1.1",
        "prettier": "^2.6.2"
    },
    "dependencies": {
        "concurrently": "^7.2.1",
        "typescript": "^4.7.3"
    },
    "packageManager": "yarn@3.2.1"
}