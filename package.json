{
    "name": "milhous",
    "version": "1.0.0",
    "description": "主站2.0",
    "main": "index.js",
    "private": true,
    "type": "module",
    "repository": {
        "type": "git",
        "url": "http://gitlab.lan.com/front-end/website-v2.git"
    },
    "workspaces": [
        "packages/*",
        "mage-cli"
    ],
    "scripts": {
        "create": "mage create",
        "launch": "mage launch",
        "setup": "lerna init --independent && lerna clean --yes && lerna bootstrap",
        "update": "yarn workspaces foreach -vt run update",
        "start": "lerna run --scope @packages/* --parallel start",
        "build": "lerna run --scope @packages/* build",
        "start:name": "cross-var yarn workspace @packages/$name run start",
        "build:name": "cross-var yarn workspace @packages/$name run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "cross-var": "^1.1.0",
        "npm-check-updates": "^12.5.9"
    },
    "packageManager": "yarn@3.2.1"
}